  },
  "scripts": {
    "start": "PORT=5555 react-scripts start",
    "start:docker": "npm run build && npm start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:5555 && electron .\"",
